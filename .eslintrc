{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/prefer-default-export": 0,
    "semi": 0,
    "object-curly-newline": 0,
    "padded-blocks": 0,
    "max-len": 0,
    "no-restricted-syntax": 0,
    "react/jsx-filename-extension": 0
  }
}
